<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="member">
    <insert id="signUp">
        insert into
            member
        values(
            #{memberId}, #{memberPwd}, #{memberName}, #{memberEnrollDate})
    </insert>
</mapper>